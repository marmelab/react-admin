{
    "name": "ra-ui-materialui",
    "version": "2.3.4",
    "description": "UI Components for react-admin with MaterialUI",
    "files": [
        "*.md",
        "lib",
        "esm",
        "src"
    ],
    "main": "lib/index",
    "module": "esm/index.js",
    "sideEffects": false,
    "authors": [
        "Fran√ßois Zaninotto",
        "Gildas Garcia"
    ],
    "repository": "marmelab/react-admin",
    "homepage": "https://github.com/marmelab/react-admin#readme",
    "bugs": "https://github.com/marmelab/react-admin/issues",
    "license": "MIT",
    "scripts": {
        "build": "rimraf ./lib && cross-env NODE_ENV=production BABEL_ENV=cjs babel --quiet ./src -d ./lib --ignore spec.js,test.js",
        "build-esm": "rimraf ./esm && cross-env NODE_ENV=production BABEL_ENV=esm babel --quiet ./src -d ./esm --ignore spec.js,test.js",
        "watch": "rimraf ./lib && cross-env NODE_ENV=production babel --watch ./src -d ./lib --ignore spec.js,test.js"
    },
    "devDependencies": {
        "babel-cli": "~6.26.0",
        "cross-env": "^5.2.0",
        "enzyme": "~3.3.0",
        "enzyme-adapter-react-16": "~1.1.1",
        "file-api": "~0.10.4",
        "ignore-styles": "~5.0.1",
        "react": "~16.3.1",
        "react-dom": "~16.3.1",
        "react-test-renderer": "~16.3.1",
        "rimraf": "^2.6.2"
    },
    "peerDependencies": {
        "react": "~16.3.0",
        "react-dom": "~16.3.0"
    },
    "dependencies": {
        "@material-ui/core": "^1.4.0",
        "@material-ui/icons": "^1.0.0",
        "autosuggest-highlight": "^3.1.1",
        "classnames": "~2.2.5",
        "inflection": "~1.12.0",
        "lodash": "~4.17.5",
        "material-ui-chip-input": "^1.0.0-beta.6",
        "papaparse": "^4.1.4",
        "prop-types": "~15.6.1",
        "ra-core": "^2.3.3",
        "react-autosuggest": "^9.4.2",
        "react-dropzone": "~4.0.1",
        "react-headroom": "^2.2.4",
        "react-redux": "~5.0.7",
        "react-router-dom": "~4.2.2",
        "react-transition-group": "^2.2.1",
        "recompose": "~0.26.0",
        "redux": "~3.7.2",
        "redux-form": "~7.4.0"
    }
}
