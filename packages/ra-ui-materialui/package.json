        "inflection": "~1.12.0",
        "jsonexport": "^3.2.0",
        "lodash": "~4.17.5",
        "prop-types": "^15.7.0",
        "query-string": "^7.1.1",
        "react-dropzone": "^12.0.4",
        "react-error-boundary": "^3.1.4",
        "react-query": "^3.32.1",
        "react-transition-group": "^4.4.1"
