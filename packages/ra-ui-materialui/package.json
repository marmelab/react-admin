        "expect": "^27.4.6",
        "file-api": "~0.10.4",
        "history": "^5.1.0",
        "ignore-styles": "~5.0.1",
        "ra-core": "^4.2.1",
        "ra-i18n-polyglot": "^4.2.1",
        "ra-language-english": "^4.2.1",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "react-hook-form": "^7.31.0",
        "react-router": "^6.1.0",
        "react-router-dom": "^6.1.0",
        "react-test-renderer": "~16.8.6",
        "rimraf": "^3.0.2",
        "typescript": "^4.4.0"
    },
    "peerDependencies": {
        "@mui/icons-material": "^5.0.1",
        "@mui/material": "^5.0.2",
        "ra-core": "^4.0.0",
