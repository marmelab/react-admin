        "enzyme": "~3.9.0",
        "enzyme-adapter-react-16": "~1.12.1",
        "file-api": "~0.10.4",
        "ignore-styles": "~5.0.1",
        "react": "~16.8.0",
        "react-dom": "~16.8.0",
        "react-test-renderer": "~16.8.6",
        "rimraf": "^2.6.3"
    },
    "peerDependencies": {
        "react": "^16.8.0",
        "react-dom": "^16.8.0"
    },
    "dependencies": {
        "@material-ui/core": "^4.0.0-beta.2",
