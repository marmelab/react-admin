        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.0",
        "react-transition-group": "^2.2.1",
        "recompose": "~0.26.0",
        "redux": "^3.7.2 || ^4.0.0",
        "redux-form": "~8.2.0"
