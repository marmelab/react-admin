    },
    "devDependencies": {
        "babel-cli": "~6.26.0",
        "cross-env": "^5.2.0",
        "rimraf": "^2.6.2"
    },
    "peerDependencies": {
        "react": "~16.3.0",
        "react-dom": "~16.3.0"
    },
    "dependencies": {
        "ra-core": "^2.2.1",
        "ra-language-english": "^2.2.0",
        "ra-ui-materialui": "^2.2.1"
    }
}
