{
    "name": "ra-tree-core",
    "version": "2.3.0",
    "description": "A treeview component without ui to use with react-admin",
    "main": "lib/index.js",
    "module": "esm/index.js",
    "sideEffects": false,
    "files": [
        "LICENSE",
        "*.md",
        "lib",
        "esm",
        "src"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/marmelab/react-admin.git"
    },
    "keywords": [
        "reactjs",
        "react",
        "react-admin",
        "admin-on-rest",
        "treeview",
        "tree"
    ],
    "author": "Gildas Garcia",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/marmelab/react-admin/issues"
    },
    "homepage": "https://github.com/marmelab/react-admin#readme",
    "scripts": {
        "build": "yarn run build-cjs && yarn run build-esm",
        "build-cjs": "rimraf ./lib && tsc",
        "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
        "watch": "rimraf ./lib && tsc --watch"
    },
    "peerDependencies": {
        "react": "~16.0.0 || ~16.1.0 || ~16.2.0 || ~16.3.0 || ~16.4.0",
        "react-dom": "~16.0.0 || ~16.1.0 || ~16.2.0 || ~16.3.0 || ~16.4.0"
    },
    "dependencies": {
        "performant-array-to-tree": "^1.1.2",
        "prop-types": "^15.6.1"
    },
    "devDependencies": {
        "babel-cli": "~6.26.0",
        "cross-env": "^5.2.0",
        "rimraf": "^2.6.2"
    }
}
