        "@types/react-redux": "^7.1.1",
        "@types/react-router": "^5.1.0",
        "@types/react-router-dom": "^5.1.0",
        "connected-react-router": "^6.5.2",
        "cross-env": "^5.2.0",
        "final-form": "^4.20.0",
        "history": "^4.7.2",
        "ignore-styles": "~5.0.1",
        "ra-core": "^3.13.2",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "react-redux": "^7.1.0",
        "react-router": "^5.1.0",
        "react-router-dom": "^5.1.0",
        "react-test-renderer": "^16.9.0 || ^17.0.0",
        "redux": "^3.7.2 || ^4.0.3"
    },
    "peerDependencies": {
        "ra-core": "^3.13.0",
        "react": "^16.9.0 || ^17.0.0",
